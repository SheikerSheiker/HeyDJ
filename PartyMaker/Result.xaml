        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PartyMaker"
        mc:Ignorable="d"
        Title="Result" Height="auto" Width="415.601">
    <Grid Margin="10" UseLayoutRounding="False">
        <DataGrid x:Name="grid" AutoGenerateColumns="True" HorizontalAlignment="Stretch" Height="auto" VerticalAlignment="Top" IsHitTestVisible="False" FontSize="20" SelectionChanged="Grid_SelectionChanged" GridLinesVisibility="None"/>
